    public void felComputationsVegA() {
        //Vegetable A
        double vegAExchange = (Double.parseDouble(jTextField_VegA_Exchange.getText()));
        choVegA = vegAExchange * 3;
        proVegA = vegAExchange * 1;
        vegAkCal = vegAExchange * 16;

        jTextField_VegA_CHO.setText(String.valueOf(choVegA));
        jTextField_VegA_Pro.setText(String.valueOf(proVegA));
        jTextField_VegA_Kcal.setText(String.valueOf(vegAkCal));
    }

    public void felComputationVegB() {

        //Vegetable B
        double vegBExchange = (Double.parseDouble(jTextField_VegB_Exchange.getText()));
        choVegB = vegBExchange * 3;
        proVegB = vegBExchange * 1;
        vegBkcal = vegBExchange * 16;

        jTextField_VegB_Cho.setText(String.valueOf(choVegB));
        jTextField_VegB_PRO.setText(String.valueOf(proVegB));
        jTextField_VegB_kcal.setText(String.valueOf(vegBkcal));
    }

    public void felComputationFruits() {

        //Fruits
        choFruit = (Integer.parseInt(jTextField_Fruit_exchange.getText())) * 10;
        kcalfruit = (Integer.parseInt(jTextField_Fruit_exchange.getText())) * 40;

        jTextField_Fruit_CHO.setText(String.valueOf(choFruit));
        jTextField_Fruit_Kcal.setText(String.valueOf(kcalfruit));
    }

    public void felComputationMilkWhole() {

        double milkWholeExchange = Double.parseDouble(milk_whole_exchange.getText());
        choMilkWhole = milkWholeExchange * 12;
        proMilkWhole = milkWholeExchange * 8;
        fatMilkWhole = milkWholeExchange * 10;
        kcalMilkWhole = milkWholeExchange * 170;

        milk_whole_cho.setText(String.valueOf(choMilkWhole));
        milk_whole_pro.setText(String.valueOf(proMilkWhole));
        milk_whole_fat.setText(String.valueOf(fatMilkWhole));
        milk_whole_kcal.setText(String.valueOf(kcalMilkWhole));
    }

    //Milk

    public void felComputationMilkLow() {
        double milklowExchange = Double.parseDouble(milk_lowfat_exchange.getText());
        choMilklow = milklowExchange * 12;
        proMilklow = milklowExchange * 8;
        fatMilklow = milklowExchange * 5;
        kcalMilklow = milklowExchange * 125;

        milk_lowfat_cho.setText(String.valueOf(choMilklow));
        milk_lowfat_pro.setText(String.valueOf(proMilklow));
        milk_lowfat_fat.setText(String.valueOf(fatMilklow));
        milk_lowfat_kcal.setText(String.valueOf(kcalMilklow));
    }

    public void felComputationMilkSkimmed() {
        double milkskimmedExchange = Double.parseDouble(milk_skimmed_exchange.getText());
        choMilkskimmed = milkskimmedExchange * 12;
        proMilkskimmed = milkskimmedExchange * 8;
        fatMilkskimmed = milkskimmedExchange * 0;
        kcalMilkskimmed = milkskimmedExchange * 80;

        milk_skimmed_cho.setText(String.valueOf(choMilkskimmed));
        milk_skimmed_pro.setText(String.valueOf(proMilkskimmed));
        milk_skimmed_fat.setText(String.valueOf(fatMilkskimmed));
        milk_skimmed_kcal.setText(String.valueOf(kcalMilkskimmed));
    }

    public void felComputationRice() {
        // Rice
        double riceExchange = Double.parseDouble(rice_exchange.getText());

        riceCho = riceExchange * 23;
        ricePro = riceExchange * 2;
        ricekcal = riceExchange * 100;

        rice_cho.setText(String.valueOf(riceCho));
        rice_pro.setText(String.valueOf(ricePro));
        rice_energykcal.setText(String.valueOf(ricekcal));

    }

    public void felComputationMeatLow() {
        double meatExchangeLow = Double.parseDouble(meat_lowfat_exchange.getText());

        meatLowpro = meatExchangeLow * 8;
        meatLowFat = meatExchangeLow * 1;
        meatLowkcal = meatExchangeLow * 41;

        meat_lowfat_pro.setText(String.valueOf(meatLowpro));
        meat_lowfat_fat.setText(String.valueOf(meatLowFat));
        meat_lowfat_kcal.setText(String.valueOf(meatLowkcal));
    }
        //Meat

    public void felComputationMeatMedium() {
        double meatExchangeMedium = Double.parseDouble(meat_mediumfat_exchange.getText());

        meatMediumpro = meatExchangeMedium * 8;
        meatMediumfat = meatExchangeMedium * 6;
        meatMediumkcal = meatExchangeMedium * 86;

        meat_mediumfat_pro.setText(String.valueOf(meatMediumpro));
        meat_mediumfat_fat.setText(String.valueOf(meatMediumfat));
        meat_mediumfat_kcal.setText(String.valueOf(meatMediumkcal));

    }

    public void felComputationMeatHigh() {
        double meatExchangeHigh = Double.parseDouble(meat_highmfat_exchange.getText());

        meatHighpro = meatExchangeHigh * 8;
        meatHighfat = meatExchangeHigh * 10;
        meatHighkcal = meatExchangeHigh * 122;

        meat_highfat_pro.setText(String.valueOf(meatHighpro));
        meat_highfat_fat.setText(String.valueOf(meatHighfat));
        meat_highfat_kcal.setText(String.valueOf(meatHighkcal));

    }

    //FAT
    public void felComputationFat() {
        double fatExchange = Double.parseDouble(fat_exchange.getText());

        fatfat = fatExchange * 5;
        fatkcal = fatExchange * 45;

        fat_fat.setText(String.valueOf(fatfat));
        fat_kcal.setText(String.valueOf(fatkcal));

    }

    //Sugar
    public void felComputationSugar() {
        double sugarExchange = Double.parseDouble(sugar_exchange.getText());

        sugarCho = sugarExchange * 5;
        sugarkcal = sugarExchange * 20;

        sugar_cho.setText(String.valueOf(sugarCho));
        sugar_kcal.setText(String.valueOf(sugarkcal));

    }

    public void felComputationTotalCHO() {
        double sumCho = choVegA + choVegB + choFruit + choMilkWhole
                + choMilklow + choMilkskimmed + riceCho + sugarCho;
        total_cho.setText(String.valueOf(sumCho));
    }

    public void felComputationTotalPro() {
        double sumPro = proVegA + proVegB + proMilkWhole + proMilklow
                + proMilkskimmed + ricePro + meatLowpro + meatMediumpro
                + meatHighpro;
        total_pro.setText(String.valueOf(sumPro));
    }

    public void felComputationTotalFat() {
        double sumFat = fatMilkWhole + fatMilklow + fatMilkskimmed
                + fatfat;
        total_fat.setText(String.valueOf(sumFat));
    }

    public void felComputationTotalkcal() {
        double sumkcal = vegAkCal + vegBkcal + kcalfruit
                + kcalMilkWhole + kcalMilklow + kcalMilkskimmed
                + ricekcal + meatLowkcal + meatMediumkcal
                + meatHighkcal + fatkcal + sugarkcal;
        total_kcal.setText(String.valueOf(sumkcal));
    }

    public void allTotal() {
        this.felComputationTotalCHO();
        this.felComputationTotalPro();
        this.felComputationTotalFat();
        this.felComputationTotalkcal();
    }


